{
  "kind": "collectionType",
  "collectionName": "product_types",
  "info": {
    "singularName": "product-type",
    "pluralName": "product-types",
    "displayName": "Product Type"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "product_type"
    }
  }
}
